      "targets": [
        {
          "datasource": {
            "type": "grafana-clickhouse-datasource",
            "uid": "clickhouse"
          },
          "format": "time_series",
          "meta": {
            "builderOptions": {
              "database": "otel",
              "fields": [
                "TimeUnix",
                "Value"
              ],
              "filters": [
                {
                  "condition": "AND",
                  "key": "MetricName",
                  "operator": "=",
                  "type": "LowCardinality(String)",
                  "value": "system.cpu.utilization"
                }
              ],
              "groupBy": [
                "ResourceAttributes['k8s.node.name']"
              ],
              "limit": 100,
              "orderBy": [
                {
                  "name": "TimeUnix",
                  "dir": "ASC"
                }
              ],
              "table": "metrics_gauge"
            }
          },
          "queryType": "",
          "rawSql": "SELECT\n  TimeUnix as time,\n  ResourceAttributes['k8s.node.name'] as node,\n  avg(Value) * 100 as cpu_percent\nFROM otel.metrics_gauge\nWHERE MetricName = 'system.cpu.utilization'\n  AND TimeUnix >= toDateTime($__from / 1000)\n  AND TimeUnix <= toDateTime($__to / 1000)\n  AND ResourceAttributes['k8s.node.name'] != ''\nGROUP BY TimeUnix, ResourceAttributes['k8s.node.name']\nORDER BY TimeUnix",
          "refId": "A"
        }
      ],
      "title": "Node CPU Utilization",
      "type": "timeseries"
    },
